{
    "name": "aurabx/dicom-data",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.2",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.0",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "Aurabx\\DicomData\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aurabx\\DicomData\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
